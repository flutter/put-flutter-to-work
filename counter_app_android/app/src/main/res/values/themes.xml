<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.newsfeed_android" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/flutter_blue_500</item>
        <item name="colorPrimaryVariant">@color/flutter_blue_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/flutter_blue_500</item>
        <item name="colorSecondaryVariant">@color/flutter_blue_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>
    <style name="Theme.Translucent.newsfeed_android" parent="Theme.newsfeed_android">
        <item name="android:windowIsTranslucent">true</item>
    </style>
</resources>